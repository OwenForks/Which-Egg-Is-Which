{
	"manifest_version": 2,
	"name": "Which Egg",
	"description": "Replace the hidden egg with the original one.",
	"version": "1.1",
	
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	
	"content_scripts": [
		{
			"matches": [ "https://dragcave.net/locations/*" ],
			"js": [ 
				"location.js",
				"content.js"
			]
		}
	],
	
	"web_accessible_resources": [
		"images/*.png",
		"images/*.gif"
	]
}